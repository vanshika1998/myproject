<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>projectservlet</servlet-name>
        <servlet-class>projectservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>projectservlet2</servlet-name>
        <servlet-class>projectservlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>contact</servlet-name>
        <servlet-class>contact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>feedback</servlet-name>
        <servlet-class>feedback</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bus</servlet-name>
        <servlet-class>bus</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>projectservlet</servlet-name>
        <url-pattern>/projectservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>projectservlet2</servlet-name>
        <url-pattern>/projectservlet2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>contact</servlet-name>
        <url-pattern>/contact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>feedback</servlet-name>
        <url-pattern>/feedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bus</servlet-name>
        <url-pattern>/bus</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
